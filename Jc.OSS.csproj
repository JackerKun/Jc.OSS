<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.0.5</PackageVersion>
        <Title>Jc.OSS</Title>
        <Description>aliyun oss helper</Description>
        <Copyright>JackerKun</Copyright>
        <PackageProjectUrl>http://jackerkun.com</PackageProjectUrl>
        <PackageLicenseUrl>http://jackerkun.com</PackageLicenseUrl>
        <PackageTags>oss</PackageTags>
        <Authors>JackerKun</Authors>
        <LangVersion>10</LangVersion>
        <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
      <DocumentationFile>bin\Debug\.netstandard2.1\Jc.OSS.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\.netstandard2.1\Jc.OSS.xml</DocumentationFile>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Aliyun.OSS.SDK.NetCore" Version="2.13.0" />
    </ItemGroup>

</Project>
